{
    "name": "cerbero/laravel-enum",
    "type": "library",
    "description": "Enum generator for Laravel.",
    "keywords": [
        "cerbero",
        "laravel-enum",
        "enum"
    ],
    "homepage": "https://github.com/cerbero90/laravel-enum",
    "license": "MIT",
    "authors": [{
        "name": "Andrea Marco Sartori",
        "email": "andrea.marco.sartori@gmail.com",
        "homepage": "https://github.com/cerbero90",
        "role": "Developer"
    }],
    "require": {
        "php": "^7.1|^8.0",
        "illuminate/console": ">=5.6",
        "illuminate/support": ">=5.6",
        "rexlabs/enum": "^2.0"
    },
    "require-dev": {
        "orchestra/testbench": ">=3.6",
        "phpunit/phpunit": ">=7.0",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Cerbero\\LaravelEnum\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cerbero\\LaravelEnum\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs src tests",
        "fix-style": "phpcbf src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "Cerbero\\LaravelEnum\\Providers\\LaravelEnumServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    }
}
